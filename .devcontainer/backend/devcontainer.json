{
    "name": "Backend Container",
    "dockerComposeFile": ["../docker-compose.yml"],
    "service": "backend",
    "workspaceFolder": "/workspace",
    "features": 
        {
            "ghcr.io/devcontainers/features/common-utils:2": {}
        },
    "postCreateCommand": "echo success",
    "customizations": {
        "vscode": {
            "settings": {
                "terminal.integrated.cwd": "/workspace/backend"
            }
        }
    }
}
